#V=voltage
#T=temperature
#DAC=dac units
#ADU=adus
# When reading, the usual input is ADU.
# When setting, the usual input is T.

[RAW]
Y(X)=X

[CARDTEMP]
T(V)=(V/3650)*1000000-273

[ADU_VOLTSx3]
V(ADU)= ((ADU-2048)/2048)*3

[ADU_VOLTSx1.5]
V(ADU)=((ADU-2048)/2048)*1.5
ADU(V)=(V/1.5)*2048+2048

[CONV+HV]
V(ADU)=(ADU-2048)*36/1232

[CONV+LV]
V(ADU)=(ADU-2048)*15/1472

[CONV-LV]
V(ADU)=(2048-ADU)*-15/1472

[LAKESHORE]
