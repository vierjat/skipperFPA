[NetComms]
module=mult_tcp
openpanel=FALSE
[mult_tcp]
cmdset=NATIVE
multport=${MULTPORT}
cmdport=${CMDPORT}
asyncport=${ASYNCPORT}
asynchost=auto
[PAN]
id=_CMDLINE_
configs=./
logs=${MYLOGDIR}
macrodir=Macros/
errno=./errno_default.h
commands=""
autoexec=INIT
modules=${SISPI_PANVIEW}/PAN/Modules
[LOG]
log=yes
file=_PANLOGPATH_/PAN.log
[GBLPATHS]
_PANLOGPATH_=../log
_LOGPATH_=../log
_MODPATH_=${SISPI_PANVIEW}/PAN/Modules
_CONFPATH_=./
[MODS]
GFITS=public/vis/GFITS_API.vi
GUNSC=public/vis/GUNSC_API.vi
LOG=public/vis/LOG_API.vi
Logical=public/vis/Logical_API.vi
TPNT=public/vis/TPNT_API.vi
[TRANSLATIONS]
LOGIC=Logical
FITS=GFITS
TEMP="DHE TP"
DISPLAY=GRTD
SHUTTER=KSHUTTER
MACRO="PAN MACRO"
SHUTDOWN="PAN SHUTDOWN"
OFFLINE="PAN OFFLINE"
NAME="PAN NAME"
ENV=PAN
